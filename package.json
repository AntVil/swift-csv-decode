{
  "name": "swift-csv-decode",
  "publisher": "AntVil",
  "displayName": "swift-csv-decode",
  "description": "A code generator tool for decoding csv in swift.",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntVil/swift-csv-decode"
  },
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "AntVil"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "workspaceContains:**/*.csv",
    "onLanguage:csv"
  ],
  "keywords": [
    "swift",
    "converter",
    "csv",
    "conversion"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swift-csv-decode.convertToSwiftDecodable",
        "title": "Convert CSV to Swift Decodable",
        "icon": {
          "dark": "./resources/open-preview-dark.svg",
          "light": "./resources/open-preview-light.svg"
        },
        "when": "editorLangId == csv"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "swift-csv-decode.convertToSwiftDecodable",
          "group": "navigation",
          "when": "editorLangId == csv"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
